// ts -> js로 컴파일하는 옵션
{
    "compilerOptions": {
      "strict": true, // typescript의 기능을 얼마나 엄격하게 
      "target": "ES5",
      "module": "CommonJS",
      "alwaysStrict": true,
      "noImplicitAny": true, // strict 하위 세부 옵션 // any라는 타입을 쓰지 못하도록 // 
      "noImplicitThis": true,
      "sourceMap": true,
      "downlevelIteration": true
    } 
  }
  